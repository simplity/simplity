#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<record xmlns="http://www.simplity.org/schema" 
	name="orders"
	moduleName="birt" 
	recordType="storage"
	okToSelectAll="true"
	keyToBeGenerated="true"
	>
	<fields>
		<field name="orderNumber" dataType="id" fieldType="primaryKey" />
		<field name="customerNumber" fieldType="parentKey" referredField="customerNumber" referredRecord="birt.customers" />
		<field name="orderDate" dataType="dateTime" />
		<field name="requiredDate" dataType="dateTime" />
		<field name="shippedDate" dataType="dateTime"  isNullable="true"/>
		<field name="status" dataType="text" />
		<field name="comments" dataType="text"  isNullable="true" />
	</fields>
</record>
