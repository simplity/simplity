#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>s</servlet-name>
    <servlet-class>${groupId}.http.Serve</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>i</servlet-name>
    <servlet-class>${groupId}.http.DefaultLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>o</servlet-name>
    <servlet-class>${groupId}.http.DefaultLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s</servlet-name>
    <url-pattern>*._s</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>i</servlet-name>
    <url-pattern>*._i</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>o</servlet-name>
    <url-pattern>*._o</url-pattern>
  </servlet-mapping>
</web-app>