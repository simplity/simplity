<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<record
	xmlns="http://www.simplity.org/schema"
	name="pet"
	moduleName="pet"
	keyToBeGenerated="true"
	defaultSheetName="pets"
	tableName="pets"
>
	<fields>
		<field
			name="petId"
			columnName="id"
			dataType="pet.id"
			fieldType="primaryKey"
			isRequired="true"
			 />
		<field
			name="ownerId"
			columnName="owner_id"
			dataType="pet.id"
			fieldType="parentKey"
			referredRecord="pet.owner"
			isRequired="true"
			 />
		<field
			name="petName"
			columnName="name"
			dataType="pet.name"
			isRequired="true"
			/>
		<field
			name="petDob"
			columnName="birth_date"
			dataType="pet.date"
			isRequired="true"
			 />
		<field
			name="petTypeId"
			columnName="type_id"
			dataType="pet.id"
			fieldType="foreignKey"
			referredRecord="pet.petType"
			isRequired="true"
			 />
	</fields>
</record>
