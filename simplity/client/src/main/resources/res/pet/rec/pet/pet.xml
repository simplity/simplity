<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dbTable
  xmlns="http://www.simplity.org/schema"
  name="pet"
  moduleName="pet"
  keyIsGenerated="true"
  defaultSheetName="pets"
  tableName="pets"
>
  <fields>
    <primaryKey
      name="petId"
      externalName="id"
      valueType="integer" />
    <parentKey
      name="ownerId"
      valueType="integer"
      externalName="owner_id"
      referredRecord="pet.owner" />
    <dbField
      name="petName"
      valueType="text"
      externalName="name"
    >
      <fieldValidation
        dataType="pet.name"
        isRequired="true" />
    </dbField>
    <dbField
      name="petDob"
      externalName="birth_date"
      valueType="date"
    >
      <fieldValidation
        dataType="pet.date"
        isRequired="true" />

    </dbField>
    <foreignKey
      name="petTypeId"
      externalName="type_id"
      valueType="integer"
      referredRecord="pet.petType"
    >
      <fieldValidation
        dataType="pet.id"
        isRequired="true" />
    </foreignKey>
  </fields>
</dbTable>
