<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dbTable
	xmlns="http://www.simplity.org/schema"
	name="visit"
	keyIsGenerated="true"
	moduleName="pet"
	defaultSheetName="visits"
	tableName="visits"
>
	<fields>
		<field
			name="visitId"
			externalName="id"
			dataType="pet.id"
			fieldType="primaryKey"
			isRequired="true" />
		<field
			name="petId"
			externalName="pet_id"
			dataType="pet.id"
			fieldType="parentKey"
			referredRecord="pet.pet"
			referredField="petId"
			isRequired="true" />
		<field
			name="visitDate"
			externalName="visit_date"
			dataType="pet.date"
			isRequired="true" />
		<field
			name="visitDescription"
			externalName="description"
			dataType="pet.description"
			isRequired="true" />
	</fields>
</dbTable>
