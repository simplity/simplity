<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dbTable
  xmlns="http://www.simplity.org/schema"
  name="visit"
  keyIsGenerated="true"
  moduleName="pet"
  defaultSheetName="visits"
  tableName="visits"
>
  <fields>
    <primaryKey
      name="visitId"
      externalName="visit_id"
      valueType="integer" />
    <parentKey
      name="petId"
      externalName="pet_id"
      referredRecord="pet.pet"
      referredField="petId"
      valueType="integer"
    >
      <fieldValidation
        dataType="pet.id"
        isRequired="true" />
    </parentKey>
    <dbField
      name="visitDate"
      externalName="visit_date"
      valueType="date"
    >
      <fieldValidation
        dataType="pet.date"
        isRequired="true" />
    </dbField>
    <dbField
      name="visitDescription"
      externalName="description"
      valueType="text">
      <fieldValidation
      dataType="pet.description"
      isRequired="true" /></dbField>
      
  </fields>
</dbTable>
