<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
  xmlns="http://www.simplity.org/schema"
  name="filterOwnersPlainJava"
  moduleName="pet"
>

  <inputData>
    <inputRecords>
      <inputRecord
        recordName="pet.owner"
        purpose="filter" />
    </inputRecords>
  </inputData>
  <processor>
    <javaProcessor
      className="org.simplity.pet.service.FilterOwners" />
  </processor>
  <outputData>
    <outputRecords>
      <outputRecord name="owners" />
      <!-- child table is to be output as child pf parent rows -->
      <outputRecord
        name="petDetails"
        parentSheetName="owners"
        linkFieldsInParentSheet="ownerId"
        linkFieldsInThisSheet="ownerId" />
    </outputRecords>
  </outputData>
</service>
